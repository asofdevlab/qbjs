{
	"name": "qbjs",
	"description": "An agnostic ORM query builder for your API Requests",
	"private": true,
	"license": "MIT",
	"author": {
		"name": "@asofdev",
		"email": "support@asof.dev"
	},
	"scripts": {
		"build": "turbo run build --filter=[HEAD^1]",
		"dev": "turbo run dev",
		"dev:doc": "pnpm --filter @qbjs/documentation dev",
		"publish:core": "pnpm --filter @qbjs/core release",
		"build:doc": "pnpm --filter @qbjs/documentation build",
		"doc:typegen": "pnpm --filter @qbjs/documentation cf-typegen",
		"deploy:doc": "pnpm --filter @qbjs/documentation cf-typegen && pnpm --filter @qbjs/documentation deploy:cf",
		"format": "turbo run format",
		"lint": "turbo run lint",
		"lint:core": "pnpm --filter @qbjs/core lint",
		"lint:client": "pnpm --filter @qbjs/client lint",
		"format:core": "pnpm --filter @qbjs/core format",
		"format:client": "pnpm --filter @qbjs/client format",
		"audit:core": "pnpm --filter @qbjs/core audit:moderate",
		"audit:client": "pnpm --filter @qbjs/client audit:moderate",
		"checktypes:core": "pnpm --filter @qbjs/core check-types",
		"checktypes:client": "pnpm --filter @qbjs/client check-types",
		"test:core": "pnpm --filter @qbjs/core test",
		"test:client": "pnpm --filter @qbjs/client test",
		"test": "turbo run test",
		"check-types": "turbo run check-types",
		"prepare": "husky",
		"clean": "turbo run clean && rm -rf node_modules",
		"clean:turbo": "rm -rf .turbo",
		"clean:deps": "rm -rf node_modules packages/*/node_modules apps/*/node_modules examples/*/node_modules integrations/*/node_modules",
		"clean:lockfiles": "rm -rf pnpm-lock.yaml",
		"reset": "pnpm clean:lockfiles clean:deps && pnpm clean:turbo && pnpm install",
		"db:generate": "pnpm --filter @examples/hono-drizzle-pg db:generate",
		"db:push": "pnpm --filter @examples/hono-drizzle-pg db:push",
		"db:studio": "pnpm --filter @examples/hono-drizzle-pg db:studio"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"husky": "^9.1.7",
		"turbo": "^2.7.3",
		"typescript": "5.9.2"
	},
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"node": ">=20"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.90.16"
	}
}
